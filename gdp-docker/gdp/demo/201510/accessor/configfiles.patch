 #--------------------------------------------------------------------
+#  GDP stuff. Since PTGDP is already used, we'll use PTGDPGCL
+#--------------------------------------------------------------------
+GDP_JAR=
+
+PTGDPGCL_DIR="$PTII/ptolemy/actor/lib/jjs/modules/GDP"
+if test ! -d "$PTGDPGCL_DIR"; then
+    PT_MSG_STATUS([$PTGDPGCL_DIR is not present,
+    so we need not find the GDP wrappers])
+else
+    AC_MSG_CHECKING([for GDP wrappers])
+    AC_ARG_WITH(gdp, [ --with-gdp=JARFILE  use GDP from JARFILE,
+                        defaults to \$PTII/GDP-0.3-1.jar],
+    GDP_JAR=$withval,
+    GDP_JAR=$PTII/lib/GDP-0.3-1.jar)
+    if test -f "$GDP_JAR"; then
+        PT_MSG_RESULT($GDP_JAR)
+        AC_MSG_WARN($GDP_JAR)
+        PTGDPGCL_JARS="$PTII/lib/jna-4.1.0.jar$CLASSPATHSEPARATOR$PTII/lib/jnaerator-0.12-shaded.jar$CLASSPATHSEPARATOR$PTII/lib/jnaerator-runtime-0.12.jar$CLASSPATHSEPARATOR$PTII/lib/GDP-0.3-1.jar"
+        PTGDPGCL_PACKAGES="ptolemy.actor.lib.jjs.modules.GDP"
+        updateClasspath "$GDP_JAR"
+        updateClasspath "$PTII/lib/jna-4.1.0.jar"
+        updateClasspath "$PTII/lib/jnaerator-0.12-shaded.jar"
+        updateClasspath "$PTII/lib/jnaerator-runtime-0.12.jar"
+        updateClasspath "$PTII/lib/GDP-0.3-1.jar"
+    fi
+fi
+
+
+#--------------------------------------------------------------------
